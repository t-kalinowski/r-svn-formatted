@USE_LIBTOOL_FALSE@LTLINK = $(SHLIB_LD) $(SHLIB_LDFLAGS) $(LDFLAGS)
@USE_LIBTOOL_TRUE@LTLINK = $(LIBTOOL) --mode=link $(SHLIB_LD) $(SHLIB_LDFLAGS) $(LDFLAGS) -rpath $(Rexeclibdir) -avoid-version
MAIN_CFLAGS = @MAIN_CFLAGS@
MAIN_FFLAGS = @MAIN_FFLAGS@
MAIN_LD = @MAIN_LD@
MAIN_LDFLAGS = @MAIN_LDFLAGS@
MAKEINDEX = @MAKEINDEX@
MKINSTALLDIRS = $(top_srcdir)/tools/mkinstalldirs
