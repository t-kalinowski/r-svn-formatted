# fixme: tcltk needs to be optional
R_PKGS = base ctest eda lqs modreg mva nls splines stepfun ts tcltk
R_XTRA_CFLAGS = @R_XTRA_CFLAGS@
R_XTRA_CPPFLAGS = -I. -I$(top_builddir)/src/include -I$(top_srcdir)/src/include
R_XTRA_FFLAGS = @R_XTRA_FFLAGS@
R_XTRA_LIBS = @R_XTRA_LIBS@
RANLIB = @RANLIB@
SHELL = /bin/sh
SHLIB_EXT = @SHLIB_EXT@
