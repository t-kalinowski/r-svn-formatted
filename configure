if test -z "${SHLIB_CXXLDFLAGS}"; then
  warn_shlib_cxxldflags="I could not determine SHLIB_CXXLDFLAGS"
  echo "configure: warning: ${warn_shlib_cxxldflags}" 1>&2
fi











