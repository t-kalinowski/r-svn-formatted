#! @PERL@
#-*- perl -*-

use File::Basename;
use Cwd;
use Getopt::Long;

@knownoptions = ("dosnames");
GetOptions (@knownoptions) || usage();

$cwd=getcwd();
if($ENV{"RHOME"}){
    $RHOME=$ENV{"RHOME"};
}
else{
    chdir(dirname($0) . "/..");
    $RHOME=getcwd();
}
chdir($cwd);



require "$RHOME/etc/buildlib.pl";

build_htmlpkglist("$RHOME/library");


sub usage {

    print "Usage:  build-htmlpkglist [--dosnames]\n";

    exit 0;
}
