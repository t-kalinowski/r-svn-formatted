msgstr "스크롤링.\n"

#: console.c:2190
msgid "  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"
msgstr "  키보드:  PgUp, PgDown, Ctrl+화살표, Ctrl+Home, Ctrl+End\n"

