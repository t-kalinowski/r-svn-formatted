#
# ${RHOME}/src/appl/Makefile

VPATH = @srcdir@
srcdir = @srcdir@
top_srcdir = @top_srcdir@

top_builddir = ../..

include $(top_builddir)/Makeconf

OBJS = \
	approx.o \
	bakslv.o \
	binning.o \
	$(BLAS) \
	ch2inv.o \
	chol.o \
	cpoly.o \
	cumsum.o \
	distance.o \
	dpoco.o \
	dpodi.o \
	dpofa.o \
	dposl.o \
	dqrdc.o \
	dqrdc2.o \
        dqrls.o \
	dqrsl.o \
	dqrutl.o \
	dsvdc.o \
	dtrsl.o \
	eigen.o \
	fortran.o \
	fft.o \
	fmin.o \
	lminfl.o \
	lowess.o \
	machar.o \
	massdist.o \
	pretty.o \
	splines.o \
	stem.o \
	strsignif.o \
	tabulate.o \
	uncmin.o \
	zeroin.o

LIB = libappl.a

all: R

R: $(LIB)

$(LIB):	$(OBJS)
	$(AR) cr $(LIB) $(OBJS)
	$(RANLIB) $(LIB)

mostlyclean: clean
clean:
	@-rm -f *.o *.a
distclean: clean
	@-rm -f Makefile
maintainer-clean: distclean
