#-*- Makefile -*-
include ../../gnuwin32/MkRules

ifdef DEBUG
 DEFS+=-g
endif

CFLAGS	= -O3 $(DEFS)
OBJS	= compat.o triostr.o trio.o trionan.o
TARGETLIB = libtrio.a

all: $(TARGETLIB)

$(TARGETLIB): $(OBJS)
	$(AR) ruv $(TARGETLIB) $(OBJS)
	$(RANLIB) $(TARGETLIB)

clean:
	rm -f *~ core $(OBJS) $(TARGETLIB)
