#-*- Makefile -*-
include ../../gnuwin32/MkRules

SOURCES = adler32.c compress.c crc32.c deflate.c gzio.c infback.c \
  inffast.c inflate.c inftrees.c trees.c uncompr.c zutil.c
OBJECTS = $(SOURCES:.c=.o)

