#! /bin/sh
# ${RHOME}/src/library/LIBSETUP

RHOME=${RHOME:-`cd ../..; pwd`}

# Set up packages (by default, all but `base' and `profile')
PKGS=`ls|sed '
/CVS/d
/LIBCLEAN/d
/LIBSETUP/d
/Makefile/d
/base/d
/profile/d
'`

# Install packages without documentation
export RHOME
sh ${RHOME}/etc/INSTALL --no-docs ${PKGS}
