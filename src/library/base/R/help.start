help.start <-
function(gui = "irrelevant", browser="netscape")
{
	file <- "$RHOME/html/index.html"
	system(paste(browser, " -remote \"openURL(", file,
		")\" 2>/dev/null || ", browser, " ", file, " &", sep = ""))
}

