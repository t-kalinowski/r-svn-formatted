# leave most of the default methods visible as people seem to call them.
export(Hershey, abline, arrows, assocplot, axTicks, axis, axis.Date,
       axis.POSIXct, barplot, barplot, barplot.default, box, boxplot,
       boxplot.default, boxplot.stats, bxp, check.options,
       close.screen, cm.colors, co.intervals, col2rgb, colors,
       colours, contour, contour.default, contourLines, coplot, curve,
       dev.control, dev.copy, dev.copy2eps, dev.cur, dev.interactive,
       dev.list, dev.next, dev.off, dev.prev, dev.print, dev.set,
       dotchart, erase.screen, filled.contour, fourfoldplot, frame,
       graphics.off, gray, grey, grid, heat.colors, hist,
       hist.default, hsv, identify, image, image.default, layout,
       layout.show, lcm, legend, lines, lines.default, locator,
       matlines, matplot, matpoints, mosaicplot, mtext, n2mfrow,
       nclass.FD, nclass.Sturges, nclass.scott, pairs, pairs.default,
       palette, panel.smooth, par, pdf, persp, pictex, pie, plot,
       plot.default, plot.design, plot.new, plot.window, plot.xy,
       points, points.default, polygon, postscript, ps.options,
       rainbow, recordPlot, rect, replayPlot, rgb, rgb2hsv, rug, screen,
       segments, split.screen, stars, stem, strheight, stripchart,
       strwidth, sunflowerplot, symbols, terrain.colors, text,
       text.default, title, topo.colors, xfig, xy.coords, xyz.coords)


S3method(barplot, default)
S3method(boxplot, default)
S3method(boxplot, formula)
S3method(contour, default)
S3method(hist, Date)
S3method(hist, default)
S3method(hist, POSIXt)
S3method(identify, default)
S3method(image, default)
S3method(lines, default)
S3method(lines, formula)
S3method(lines, histogram)
S3method(mosaicplot, default)
S3method(mosaicplot, formula)
S3method(pairs, default)
S3method(pairs, formula)
S3method(persp, default)
S3method(plot, data.frame)
S3method(plot, Date)
S3method(plot, default)
S3method(plot, factor)
S3method(plot, formula)
S3method(plot, histogram)
S3method(plot, POSIXct)
S3method(plot, POSIXlt)
S3method(plot, table)
S3method(points, default)
S3method(points, formula)
S3method(print, recordedplot)
S3method(text, default)

if(.Platform$OS.type == "unix") {
export(GNOME, X11, bitmap, dev2bitmap, gnome, jpeg, png, quartz, x11)
}

if(.Platform$OS.type == "windows") {
export(X11, bitmap, bmp, bringToTop, dev2bitmap, jpeg, png, savePlot,
       win.graph, win.metafile, win.print, windows, x11)

S3method("[", SavedPlots)
S3method(print, SavedPlots)
}
